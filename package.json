{
  "name": "@gigalogy/vue-intercom",
  "description": "A wrapper for Intercom",
  "version": "0.1.0",
  "author": "Md Moin Uddin <moin@gigalogy.com>",
  "keywords": [
    "vue",
    "vuejs",
    "javascript",
    "vue-component",
    "intercom"
  ],
  "license": "MIT",
  "files": [
    "dist/*.js"
  ],
  "main": "dist/vue-intercom.umd.js",
  "module": "dist/vue-intercom.esm.js",
  "unpkg": "dist/vue-intercom.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gigalogy/vue-intercom.git"
  },
  "bugs": {
    "url": "https://github.com/gigalogy/vue-intercom/issues"
  },
  "homepage": "https://vue-intercom.gigalogy.com.bd/",
  "scripts": {
    "commit": "git cz",
    "dev": "npm run build:es -- --watch",
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:doc": "cd doc-src && npm install && npm run build",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "send:coverage": "cat ./tests/coverage/lcov.info | codacy-coverage",
    "lint": "vue-cli-service lint --ext .js,.vue src tests/unit/* build/*",
    "lint:fix": "npm run lint --fix",
    "test:client": "NODE_ENV=test vue-cli-service test:unit",
    "prepublishOnly": "npm run lint:fix && npm run test:client && npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.7.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "babel-jest": "^28.1.3",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "codacy-coverage": "^3.4.0",
    "commitizen": "^4.1.2",
    "core-js": "^3.6.5",
    "cssnano": "^5.1.13",
    "cz-customizable": "^6.9.1",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "handlebars": "^4.7.7",
    "husky": "^4.2.5",
    "mockdate": "^2.0.2",
    "node-sass": "^7.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.8.0",
    "postcss-simple-vars": "^6.0.3",
    "rollup": "^2.78.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-visualizer": "^5.7.1",
    "rollup-plugin-vue": "^4.7.2",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "vue": "^2.7.8",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.7.8"
  },
  "directories": {
    "test": "tests"
  }
}
